files:
    "/etc/nginx/conf.d/proxy.conf" :
        mode: "000755"
        owner: root
        group: root
        content: |
           client_max_body_size 10M;
           server {
             server_name admin1.neetprep.com;
             rewrite ^ https://newadmin.neetprep.com$request_uri? permanent;
           }
