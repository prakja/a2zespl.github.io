<%= semantic_form_for question, :url => "/admin/qs/#{question.id}", :remote => true do |f| %>
  <input type="hidden" name="authenticity_token" value="#{form_authenticity_token.to_s}">
  <%= f.inputs 'Question Set' do %>
    <%= f.input :questionSet_ids, as: :tags, collection: Test.where(id: Question::QUESTION_SET_TEST_IDS), display_name: :name, input_html: {id: "question_set" + question.id.to_s} %>
  <% end %>
  <%= f.actions %>
<% end %>
