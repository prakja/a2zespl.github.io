<div style="margin: 20px;">
  <form action="/user_analytics/userData" method="post">
    <%= hidden_field_tag :authenticity_token, form_authenticity_token %>
    <label for="emails">Emails:</label><textarea name="emails"><%=params[:emails]%></textarea>
    <label for="Phone Numbers">Phone Numbers:</label><textarea name="phone_numbers"><%=params[:phone_numbers]%></textarea>
    <button style="margin-top: 20px;" type="submit">Find</button>
  </form>
  <h3>Users List</h3>
  <table>
    <tr>
      <th>Id</th>
      <th>Email</th>
      <th>Email2</th>
      <th>Phone</th>
      <th>Phone2</th>
      <th>Course Ids</th>
      <th>Questions</th>
      <th>Successful Payments</th>
    </tr>
    <% if @users && @users.length > 0 %>
      <% @users.each do |user| %>
        <tr>
          <td><%= user.id %></td>
          <td><%= user.email %></td>
          <td><%=user.userProfileEmail[0]%></td>
          <td><%= user.phone %></td>
          <td><%=user.userProfilePhone[0]%></td>
          <td><%= user.courseIds %></td>
          <td><%= user.answerCount %></td>
          <td><%= link_to "User Payments", admin_payments_path(q: {userId_eq: user.id}, order: 'createdAt_desc', scope: 'successful_payments') %></td>
        </tr>
      <% end %>
    <% end %>
  </table>
</div>
